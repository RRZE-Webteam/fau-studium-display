{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",

    "jsx": "react-jsx",
    "jsxImportSource": "@wordpress/element",

    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,

    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*"]
}